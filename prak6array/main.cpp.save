#include <iostream>

using namespace std;

int main()
{
    int x;
    cout<<"Jumlah Array = ";
    cin>>x;
 
    int a[x];
    for (int i=1; i<=x; i++)
    {
        cout<<"Masukan Data ke "<<i<<" = ";
        cin>>a[i];
    }


    cout<<"ISI ARRAY = \n";
    for (int i=1; i<=x; i++)
    {
        cout<<a[i]<<", ";
    }

    cout<<"\nISI ARRAY GANJIL = \n";
    for (int i=1; i<=x; i++)
    {
        if (a[i]%2==1)
        {
            cout<<a[i]<<", ";
        }
    }


    cout<<"\nISI ARRAY GENAP = \n";
    for (int i=1; i<=x; i++)
    {

        if (a[i]%2==0)
        {
            cout<<a[i]<<", ";
        }
    }
}
